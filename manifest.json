{
	"name": "Stock Info",
	"description": "Find details of stock based on name dropped on extension",
	"version": "1.0",
	"manifest_version": 3,
	"icons": {
		"128": "./icon128.png",
		"48": "./icon48.png",
		"16": "./icon16.png"
	},
	"action": {
		"default_icon": "./icon16.png",
		"default_popup": "popup.html"
	},
	"permissions": ["contextMenus", "tabs"],
	"background": {
		"service_worker": "background.js"
	}
}
